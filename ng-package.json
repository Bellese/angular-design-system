{
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "allowedNonPeerDependencies": [
    "angular",
    "rxjs",
    "sw-toolbox",
    "zone.js",
    "ngx-pagination",
    "npm",
    "moment",
    "ngx-charts",
    "d3",
    "tsickle",
    "ngx-clipboard",
    "@fortawesome/fontawesome-svg-core",
    "@fortawesome/free-regular-svg-icons",
    "@fortawesome/free-solid-svg-icons",
    "@material-extended/mde",
    "ngx-toastr",
    "ngx-tippy-wrapper"
  ],
  "lib": {
    "entryFile": "public_api.ts",
    "umdModuleIds": {
      "@fortawesome/angular-fontawesome": "angularFontawesome",
      "@fortawesome/free-regular-svg-icons": "freeRegularSvgIcons",
      "@fortawesome/free-solid-svg-icons": "freeSolidSvgIcons",
      "@material-extended/mde": "mde",
      "@ngx-formly/core": "ngxFormlyCore",
      "@swimlane/ngx-charts": "ngxCharts",
      "moment": "moment",
      "ngx-clipboard": "ngxClipboard",
      "ngx-pagination": "ngxPagination",
      "ngx-tippy-wrapper": "ngxTippyWrapper",
      "ngx-toastr": "ngxToaster"
    }
  }
}
